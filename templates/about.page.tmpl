{{template "base" .}}

{{define "content"}}

<div class="container">
    <div class="row">
        <div class="col">
            <h1>This is about page</h1>
            <p>This came from template: {{index .StringMap "test"}}</p>

            <p>
              {{if ne (index .StringMap "remote_ip") ""}}
                Your remote IP is: {{index .StringMap "remote_ip"}}
              {{else}}
                I don't know your IP address yet. Visit the <a href="/">Home page</a> to find out.
              {{end}}

            </p>
        </div>
    </div>
</div>

{{end}}
